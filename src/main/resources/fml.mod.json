{
  "schemaVersion": 1,
  "id": "mite_creation",
  "version": "${version}",
  "depends": {
    "mite": "~1.6.4",
    "fishmodloader": ">=3.3.2",
    "rusted_iron_core": ">=1.1.2",
    "fabric-language-kotlin": ">=1.11.0+kotlin.2.0.0"
  },
  "environment": "*",
  "name": "MITE Creation",
  "description": "An extended MITE content mod",
  "authors": [
    "Xy_Lose",
    "Rusted Iron Ingot"
  ],
  "entrypoints": {
    "preLaunch": [
      "mod.mitecreation.EarlyRiser"
    ],
    "main": [
      "mod.mitecreation.MITECreationMod"
    ]
  },
  "mixins": [
    "mitecreation.mixins.json"
  ],
  "accessWidener": "MITECreation.accesswidener",
  "custom": {
    "loom:injected_interfaces": {
      "net/xiaoyu233/fml/reload/event/ItemRegistryEvent": ["huix/faustspotion/injected_interfaces/IIItemRegistryEvent"],
      "net/minecraft/EntityPlayer": ["huix/faustspotion/injected_interfaces/IIPlayer"]
    }
  }
}